trigger CampId on Contact (before insert, before update) {
    private String Nombre;
    private String Idprocontacto;
    private String Mail;
    
    for(Contact c:Trigger.new){
       Nombre = c.LastName;
        Idprocontacto = c.idprocontacto__c;
        Mail = c.Email;
        
    }
    
}

// In Apex anonymous
